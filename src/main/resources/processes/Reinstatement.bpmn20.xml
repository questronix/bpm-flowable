  <process id="startReinstatement" name="Reinstatement" isExecutable="true">
    <startEvent id="flowStart" name="Start"></startEvent>
    <userTask id="createPolicy" name="Pre-screening process" flowable:assignee="${Username}" flowable:candidateGroups="csa">
      <extensionElements>
        <modeler:initiator-can-complete xmlns:modeler="http://flowable.org/modeler"><![CDATA[false]]></modeler:initiator-can-complete>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="isCompleteAndValid" name="Check" default="f5"></exclusiveGateway>
    <serviceTask id="sendEmail" name="Send Email notification" flowable:class="org.prulife.com.delegates.SendEmail"></serviceTask>
    <endEvent id="flowEnd" name="End"></endEvent>
    <serviceTask id="sendToLifeAsia" name="Send to data to Life Asia" flowable:class="org.prulife.com.delegates.SendToLifeAsia"></serviceTask>
    <userTask id="evaluatePolicy" name="Processor Evaluation and Review" flowable:assignee="${Username}" flowable:candidateGroups="processor">
      <extensionElements>
        <modeler:initiator-can-complete xmlns:modeler="http://flowable.org/modeler"><![CDATA[false]]></modeler:initiator-can-complete>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="toEndorse" name="to be endorsed?" default="f9"></exclusiveGateway>
    <sequenceFlow id="f1" name="request" sourceRef="flowStart" targetRef="createPolicy"></sequenceFlow>
    <sequenceFlow id="f6" name="end" sourceRef="sendEmail" targetRef="flowEnd"></sequenceFlow>
    <exclusiveGateway id="beyondLimit" name="Beyond Authority?" default="f10"></exclusiveGateway>
    <endEvent id="end" name="end"></endEvent>
    <sequenceFlow id="f7" sourceRef="evaluatePolicy" targetRef="toEndorse"></sequenceFlow>
    <sequenceFlow id="f4" name="create task" sourceRef="sendToLifeAsia" targetRef="evaluatePolicy"></sequenceFlow>
    <sequenceFlow id="f13" name="assign to uc or aml" sourceRef="referToUCorAML" targetRef="end"></sequenceFlow>
    <sequenceFlow id="f12" name="end" sourceRef="processorApproval" targetRef="end"></sequenceFlow>
    <sequenceFlow id="f2" name="check" sourceRef="createPolicy" targetRef="isCompleteAndValid"></sequenceFlow>
    <sequenceFlow id="f5" name="!valid or !complete" sourceRef="isCompleteAndValid" targetRef="sendEmail"></sequenceFlow>
    <sequenceFlow id="f3" name="valid &amp; complete" sourceRef="isCompleteAndValid" targetRef="sendToLifeAsia">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isCompleteAndValid == true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="f10" name="no" sourceRef="beyondLimit" targetRef="processorApproval"></sequenceFlow>
    <serviceTask id="referToUCorAML" name="Refer to UC or AML" flowable:class="org.prulife.com.delegates.ReferToUCorAML"></serviceTask>
    <serviceTask id="processorApproval" name="Set Approval" flowable:class="org.prulife.com.delegates.SetProcessApproval"></serviceTask>
    <sequenceFlow id="f11" name="yes (pass to asm)" sourceRef="beyondLimit" targetRef="end">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${beyondAuthority==true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="f9" name="no hit" sourceRef="toEndorse" targetRef="beyondLimit"></sequenceFlow>
    <sequenceFlow id="f8" name="hit" sourceRef="toEndorse" targetRef="referToUCorAML">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${hit==true}]]></conditionExpression>
    </sequenceFlow>
  </process>
